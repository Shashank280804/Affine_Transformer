{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "app.js",
        "use": "@vercel/static"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.py"  
      },
      {
        "src": "/(.*)",
        "dest": "/index.html" 
      }
    ]
  }
  